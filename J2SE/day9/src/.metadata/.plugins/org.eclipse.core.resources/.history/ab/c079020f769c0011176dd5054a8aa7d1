package shpcart;

public class Book {
	
	private String name;
	private String author;
	private String desc;
	private double price;
	private double quantity;
	
	public Book(String name, String author, String desc, double price,
			double quantity) {
		super();
		this.name = name;
		this.author = author;
		this.desc = desc;
		this.price = price;
		this.quantity = quantity;
	}

	
}
