package Pbank;
import java.text.SimpleDateFormat;
import java.util.*;
public class Account {
	private int id;
	private String name;
	private String type;
	private int balance;
	private Date doc;
	private SimpleDateFormat sdf;
	
	public Account(int id, String name, String type, int balance, Date doc) {
		super();
		this.id = id;
		this.name = name;
		this.type = type;
		this.balance = balance;
		this.doc = doc;
		sdf=new SimpleDateFormat("dd-mmm-yyyy");
	}
	
}
